					<a2 class=next>&gt; <a class=next href=../{{ leeet.num | int  + 1 }}/>{{ leeet[leeet.num | int ].title|safe }}</a></a2>
				</div>
			{% endif %}
			
			<div style="float: left; display: inline;" id=evildiv> 
				{% if 1 < leeet.num %}
				<a href="../1/" class="13337">restart</a><a class="small 13337">&nbsp;</a> <a id=goback href="../{{leeet.num - 1}}/" class="13337">go back</a>
